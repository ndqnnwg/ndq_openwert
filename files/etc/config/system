
config system
	option hostname 'OpenWrt'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option timezone 'CST-8'
	option zonename 'Asia/Shanghai'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.ntp1.aliyun.com'
	list server '1.ntp2.aliyun.com'
	list server '2.ntp3.aliyun.com'
	list server '3.ntp4.aliyun.com'

